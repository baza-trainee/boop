generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         String    @id @default(cuid())
  email      String    @unique
  password   String?
  createdAt  DateTime  @default(now())
  updatedAt  DateTime  @updatedAt
}
 
model Photo {
  id          Int    @id @default(autoincrement())
  location    String
  imageUrl    String
  imageId     String
  createdAt  DateTime  @default(now())
 }