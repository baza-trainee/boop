generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         String    @id @default(cuid())
  email      String    @unique
  password   String?
  createdAt  DateTime  @default(now())
  updatedAt  DateTime  @updatedAt
}
 
model Test{
  id       Int    @id @default(autoincrement())
  title    String
  text     String
 }